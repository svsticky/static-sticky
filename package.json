{
  "name": "static-sticky",
  "description": "Sticky's website (svsticky.nl)",
  "version": "1.0.1",
  "author": "Commit (https://github.com/orgs/svsticky/teams/commit)",
  "dependencies": {
    "atomic-layout": "^0.12.5",
    "bufferutil": "^4.0.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.19.9",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-intl": "^0.3.2",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-styled-components": "^3.1.18",
    "gatsby-source-contentful": "^2.1.84",
    "markdown-to-jsx": "^6.10.3",
    "react": "^16.9.0",
    "react-currency-formatter": "^1.1.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-pager": "^1.3.3",
    "react-slideshow-image": "^1.3.3",
    "react-transition-group": "^4.3.0",
    "sanitize-html": "^1.21.1",
    "semantic-ui-react": "^0.88.2",
    "slash": "^3.0.0",
    "styled-components": "^5.0.0",
    "typescript": "^3.7.5",
    "utf-8-validate": "^5.0.2"
  },
  "keywords": [
    "static",
    "website",
    "studyassociation",
    "sticky"
  ],
  "license": "MIT",
  "scripts": {
    "//": "Run configurations to build & watch the semantic folder",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && npm run build-semantic && gulp watch",
    "build": "npm run build-semantic && gatsby build",
    "develop": "gatsby develop",
    "format": "npx prettier --write {*,src/**/*}.{js,jsx}",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.27",
    "gulp": "^4.0.2",
    "husky": "^4.2.1",
    "less": "^3.10.3",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "fomantic-ui": "^2.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svsticky/static-sticky"
  },
  "husky": {
    "hooks": {
      "//": "Best thing i could find that worked on WSL with the Git from Windows",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*,src/**/*}.{js,jsx}": [
      "npx prettier --write {*,src/**/*}.{js,jsx}",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
