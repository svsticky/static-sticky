{
  "name": "static-sticky",
  "description": "Sticky's website (svsticky.nl)",
  "version": "1.0.1",
  "author": "Commit (https://github.com/orgs/svsticky/teams/commit)",
  "dependencies": {
    "acorn": "^6.1.1",
    "atomic-layout": "^0.7.1",
    "dotenv": "^6.2.0",
    "gatsby": "^2.1.19",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-source-contentful": "^2.0.33",
    "markdown-to-jsx": "^6.9.1",
    "react": "^16.8.3",
    "react-currency-formatter": "^1.1.0",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-pager": "^1.3.3",
    "react-slideshow-image": "^1.1.2",
    "react-transition-group": "^2.6.0",
    "sanitize-html": "^1.20.0",
    "semantic-ui-react": "^0.85.0",
    "slash": "^2.0.0",
    "styled-components": "^4.1.3"
  },
  "keywords": [
    "static",
    "website",
    "studyassociation",
    "sticky"
  ],
  "license": "MIT",
  "scripts": {
    "//": "Run configurations to build & watch the semantic folder",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && npm run build-semantic && gulp watch",
    "build": "npm run build-semantic && gatsby build",
    "develop": "gatsby develop",
    "format": "npx prettier --write {*,src/**/*}.{js,jsx}",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.8",
    "gulp": "^3.9.1",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "semantic-ui": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svsticky/static-sticky"
  },
  "husky": {
    "hooks": {
      "//": "Best thing i could find that worked on WSL with the Git from Windows",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*,src/**/*}.{js,jsx}": [
      "npx prettier --write {*,src/**/*}.{js,jsx}",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
