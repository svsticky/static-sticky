{
  "name": "static-sticky",
  "description": "Sticky's website (svsticky.nl)",
  "version": "1.0.1",
  "author": "Commit (https://github.com/orgs/svsticky/teams/commit)",
  "dependencies": {
    "atomic-layout": "^0.9.8",
    "dotenv": "^8.1.0",
    "gatsby": "^2.15.24",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-styled-components": "^3.1.6",
    "gatsby-source-contentful": "^2.1.42",
    "markdown-to-jsx": "^6.10.3",
    "react": "^16.9.0",
    "react-currency-formatter": "^1.1.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-pager": "^1.3.3",
    "react-slideshow-image": "^1.3.0",
    "react-transition-group": "^4.3.0",
    "sanitize-html": "^1.20.1",
    "semantic-ui-react": "^0.88.1",
    "slash": "^3.0.0",
    "styled-components": "^4.4.0"
  },
  "keywords": [
    "static",
    "website",
    "studyassociation",
    "sticky"
  ],
  "license": "MIT",
  "scripts": {
    "//": "Run configurations to build & watch the semantic folder",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && npm run build-semantic && gulp watch",
    "build": "npm run build-semantic && gatsby build",
    "develop": "gatsby develop",
    "format": "npx prettier --write {*,src/**/*}.{js,jsx}",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.8",
    "gulp": "^3.9.1",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "semantic-ui": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svsticky/static-sticky"
  },
  "husky": {
    "hooks": {
      "//": "Best thing i could find that worked on WSL with the Git from Windows",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*,src/**/*}.{js,jsx}": [
      "npx prettier --write {*,src/**/*}.{js,jsx}",
      "git add"
    ]
  },
  "browserslist": [
      "defaults"
  ]
}
